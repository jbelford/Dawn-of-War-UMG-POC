{
  "name": "dow-umg",
  "productName": "Dawn of War: UMG",
  "version": "0.0.1",
  "description": "My Electron application description",
  "main": "./.webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": "jackb",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "test_forge"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-hot-loader": "4.1.0",
    "bootstrap": "4.2.1",
    "electron-squirrel-startup": "1.0.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-hot-loader": "4.6.3",
    "tslib": "1.4.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.30",
    "@electron-forge/maker-deb": "6.0.0-beta.30",
    "@electron-forge/maker-rpm": "6.0.0-beta.30",
    "@electron-forge/maker-squirrel": "6.0.0-beta.30",
    "@electron-forge/maker-zip": "6.0.0-beta.30",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.22",
    "css-loader": "^2.1.0",
    "electron": "4.0.1",
    "electron-devtools-installer": "^2.2.4",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.1.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  }
}